{
	"name": "keyes",
	"version": "1.0.0",
	"private": false,
	"description": "A tiny package for intercepting keypresses and firing callbacks.",
	"repository": "https://github.com/Renddslow/keyes",
	"license": "MIT",
	"author": "Matt McElwee <mubatt@wyopub.com>",
	"main": "dist/index.js",
	"scripts": {},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,css,json,md,ts,tsx}": [
			"prettier --write"
		]
	},
	"prettier": "@dmsi/prettier-config",
	"ava": {
		"babel": false,
		"compileEnhancements": false,
		"concurrency": 4,
		"extensions": [
			"ts"
		],
		"files": [
			"src/**/*.test.ts"
		],
		"require": [
			"ts-node/register"
		],
		"timeout": "1m"
	},
	"devDependencies": {
		"@dmsi/prettier-config": "^1.3.0",
		"@types/node": "^17.0.25",
		"ava": "^4.2.0",
		"husky": "^7.0.4",
		"lint-staged": "^12.3.8",
		"prettier": "^2.6.2",
		"ts-node": "^10.7.0",
		"typescript": "^4.6.3"
	}
}
